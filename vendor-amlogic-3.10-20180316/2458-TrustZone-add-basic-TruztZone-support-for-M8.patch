+//        dev_name = "vdin1";
+//        status = "ok";
+//        reg = <0x1e400000 0x04000000>;
+//        irq = <117>;
+//        vdin_id = <1>;
+//	};
+
+/// ***************************************************************************************
+///	-	DISP&MM-AMLVIDEO2
+//$$ MODULE = "DISP&MM-AMLVIDEO2"
+//$$ DEVICE="amlvideo2"
+//$$ L2 PROP_STR = "status"
+//$$ L3 PROP_U32 2 ="reg"
+//    amlvideo2{
