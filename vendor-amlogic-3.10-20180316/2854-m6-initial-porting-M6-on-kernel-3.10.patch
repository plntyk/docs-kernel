+	unsigned int vid_cntl = aml_read_reg32(P_HHI_VID_PLL_CNTL);
+	unsigned long parent_clk;
+	unsigned od_fb,od_hdmi,od_ldvs,M,N;
+	parent_clk = clk_get_rate(clkdev->parent);
+	parent_clk /= 1000000;
+	od_ldvs = (vid_cntl>>16)&3;
+	od_hdmi = (vid_cntl>>18)&3;
+	od_fb = (vid_cntl>>20)&3;
+	M = vid_cntl&0x3FF;
+	N = (vid_cntl>>10)&0x1F;
+	if(od_hdmi == 0)
+		od_hdmi = 1;
+	else if(od_hdmi == 1)
+		od_hdmi = 2;
+	else if(od_hdmi == 2)
+		od_hdmi = 4;
+	if(od_fb == 0)
+		od_fb = 1;
+	else if(od_fb == 1)
+		od_fb = 2;
+	else if(od_fb == 2)
+		od_fb = 4;
+
+	clk = parent_clk * M * od_fb / N;
+	clk /= od_hdmi;
+	clk *= 1000000;
+	return clk;
+}
