+static unsigned long clk_get_rate_vid2(struct clk * clkdev)
+{
+	unsigned long clk;
+	unsigned int viid_cntl = aml_read_reg32(P_HHI_VIID_PLL_CNTL);
+	unsigned long parent_clk;
+	unsigned od,M,N;
+	parent_clk = clk_get_rate(clkdev->parent);
+	parent_clk /= 1000000;
+	od = (viid_cntl>>16)&3;
+	M = viid_cntl&0x1FF;
+	N = (viid_cntl>>9)&0x1F;
+	if(od == 0)
+		od = 1;
+	else if(od == 1)
+		od = 2;
+	else if(od == 2)
+		od = 4;
+	clk = parent_clk * M / N;
+	clk /= od;
+	clk *= 1000000;
+	return clk;
+}
