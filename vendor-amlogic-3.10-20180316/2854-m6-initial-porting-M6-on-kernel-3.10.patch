+    printk(KERN_INFO "meson_pm_probe done 0x%x %d!\n", (unsigned)meson_sram_suspend, meson_cpu_suspend_sz);
+#else
+    printk(KERN_INFO "meson_pm_probe done !\n");
+#endif
+    return 0;
+}
+
+static int __exit meson_pm_remove(struct platform_device *pdev)
+{
