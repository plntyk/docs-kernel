diff --git a/drivers/amlogic/nand/Kconfig b/drivers/amlogic/nand/Kconfig
deleted file mode 100755
index f6fdd4cb3ade..000000000000
--- a/drivers/amlogic/nand/Kconfig
+++ /dev/null
@@ -1,65 +0,0 @@
-#
-# Amlogic NAND device configuration
-#
-
-menu "Meson NAND Device Support"
-
-config AM_NAND
-	bool "Meson NAND flash device"
-	default n
-	select MTD_NAND
-	select MTD_PARTITIONS
-	select MTD_BLOCK
-	select MTD_CHAR
-	select MTD
-	help
-	  Support for NAND flash on Amlogic Meson platforms
-
-config AM_NAND_RBPIN
-	bool "Meson NAND RB bit support"
-	depends on AM_NAND
-	depends on ARCH_MESON3 || ARCH_MESON6
-	default y
-	help
-	  Support NAND RB bit
-
-config AML_NAND_ENV
-	bool "Meson NAND environment variable device (dev/nand_env) support"
-	depends on AM_NAND
-	depends on ARCH_MESON3 || ARCH_MESON6
-	default y
-	help
-	  Support NAND environment variable device
-
-config AML_NAND_KEY
-	bool "Meson key data store in nand device(dev/nand_key) support"
-	depends on AM_NAND
-	depends on ARCH_MESON3 || ARCH_MESON6
-	default n
-	help
-	  Support   key variable store in NAND device
-	  
-config NAND_KEY_BLOCK_NUM
-	int "config nand key area block number"
-	depends on AML_NAND_KEY
-	range 2  16
-	default 8
-	help
-		config nand key block number, the number must be even number
-
-config AML_NFTL
-	tristate "Meson NAND FTL support"
-	depends on AM_NAND
-	default m
-	help
-	  Support NAND Flash Translation Layer
-	  
-config AML_NFTL_NEW
-	tristate "Meson NAND NEW_FTL support"
-	depends on AM_NAND
-	default n
-	help
-	  Support NAND Flash New Translation Layer
-
-endmenu
-        
\ No newline at end of file
diff --git a/drivers/amlogic/nand/Makefile b/drivers/amlogic/nand/Makefile
deleted file mode 100755
index 5847286e8c4b..000000000000
--- a/drivers/amlogic/nand/Makefile
+++ /dev/null
@@ -1,16 +0,0 @@
-#
-# Makefile for the Amlogic NAND drivers.
-#
-obj-$(CONFIG_AM_NAND)		+= aml_nand.o
-obj-$(CONFIG_ARCH_MESON1)	+= m1_nand.o
-obj-$(CONFIG_ARCH_MESON2)	+= m2_nand.o
-obj-$(CONFIG_ARCH_MESON3)	+= m3_nand.o
-obj-$(CONFIG_ARCH_MESON6)	+= m3_nand.o
-obj-$(CONFIG_ARCH_MESON6TV)	+= m3_nand.o
-#obj-$(CONFIG_AML_NFTL)		+= aml_nftl/
-ifeq ($(CONFIG_AML_NFTL_NEW), y)              
-	obj-$(CONFIG_AML_NFTL_NEW)	+= aml_nftl_new/
-else                                          
-	obj-$(CONFIG_AML_NFTL)		+= aml_nftl/      
-endif
-obj-$(CONFIG_AML_NAND_KEY)	+=nand_key.o  
\ No newline at end of file
