-			break;
-		case 1:
-			info->ce_sel=CE1;
-			break;
-		case 2:
-			info->ce_sel=CE2;
-			break;
-		case 3:	
-			info->ce_sel=CE3;
-			break;
-		default:
-			BUG();
-	}
-
-	NFC_SEND_CMD(info->ce_sel|IDLE | 1); 
-	
-	return ;
