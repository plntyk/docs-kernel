-static void ntd_blktrans_request(struct request_queue *rq)
-{
-    struct ntd_blktrans_dev *dev;
-    struct request *req = NULL;
-
-    dev = rq->queuedata;
-
-    if (!dev)
-        while ((req = blk_fetch_request(rq)) != NULL)
-            __blk_end_request_all(req, -ENODEV);
-    else {
-        dev->bg_stop = true;
-        wake_up_process(dev->thread);
-    }
