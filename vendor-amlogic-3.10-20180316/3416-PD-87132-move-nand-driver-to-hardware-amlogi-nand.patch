-	set_mio_mux(6,0x7ff);
-	clear_mio_mux(7,CM7);
-
-	switch (chipnr) {
-		case -1:
-			chip->cmd_ctrl(mtd, NAND_CMD_NONE, 0 | NAND_CTRL_CHANGE);
-			set_mio_mux(1,CM0);
-			clear_mio_mux(1,SM0);
