+{
+    VDEC1_CLOCK_OFF();
+}
+
+void vdec2_clock_enable(void)
+{
+    VDEC2_CLOCK_OFF();
+    VDEC2_255M();
+    VDEC2_CLOCK_ON();
+    clock_level[VDEC_2] = 0;
+}
+
+void vdec2_clock_hi_enable(void)
+{
+    VDEC2_CLOCK_OFF();
+    VDEC2_364M();
+    VDEC2_CLOCK_ON();
+    clock_level[VDEC_2] = 1;
+}
+
+void vdec2_clock_on(void)
