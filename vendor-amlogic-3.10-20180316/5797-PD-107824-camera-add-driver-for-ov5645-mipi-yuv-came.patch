+	int i = 0;
+	/* int rc = -1; */
+	if (!res_param->reg_script) {
+		pr_info("error, resolution reg script is NULL\n");
+		return -1;
+	}
+	while (1) {
+		if (res_param->reg_script[i].val == 0xff &&
+				res_param->reg_script[i].addr == 0xffff) {
+			pr_info("setting resolutin param complete\n");
