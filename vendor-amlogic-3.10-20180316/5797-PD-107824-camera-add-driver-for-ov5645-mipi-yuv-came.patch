+		if ((i2c_put_byte(client, res_param->reg_script[i].addr,
+					res_param->reg_script[i].val)) < 0) {
+			pr_info("fail in setting resolution param. i=%d\n", i);
+			break;
+		} else {
+		}
+		i++;
+	}
+	dev->cur_resolution_param = res_param;
+	set_flip(dev);
+
+	return 0;
