+void ov5645_reset(struct i2c_client *client)
+{
+	pr_info("%s\n", __func__);
+	i2c_put_byte(client, 0x3103, 0x11);/* ; PLL clock selection */
+	msleep(5);
+	i2c_put_byte(client, 0x3008, 0x82);
+	/* msleep(10); //delay(2);		//; delay 2ms */
+}
