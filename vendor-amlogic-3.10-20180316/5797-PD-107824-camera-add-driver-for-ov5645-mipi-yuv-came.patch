+unsigned char v4l_2_ov5645(int val)
+{
+	int ret = val/0x20;
+	if (ret < 4)
+		return ret*0x20+0x80;
+	else if (ret < 8)
+		return ret*0x20+0x20;
+	else
+		return 0;
+}
