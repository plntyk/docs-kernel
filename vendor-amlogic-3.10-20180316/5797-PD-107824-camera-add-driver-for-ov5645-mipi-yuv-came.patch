+static resolution_size_t get_res_size(const char *res_str)
+{
+	resolution_size_t ret = SIZE_NULL;
+	struct res_item *item;
+	int i;
+	if (!res_str)
+		return SIZE_NULL;
+	for (i = 0; i < ARRAY_SIZE(res_item_array); i++) {
